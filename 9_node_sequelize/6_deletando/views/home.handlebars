<h1>Usu√°rios cadastrados</h1>
<div class="container">
    {{#each users}}
        <div class="item">
            <p style="text-transform: capitalize;">{{this.name}}</p>
            <a href="/users/{{this.id}}">Ver mais</a>
            <form action="/users/delete/{{this.id}}"  method="post">
                <input type="submit" value="excluir" class="delete-btn">
            </form>
        </div>
    {{/each}}
</div>